<% @page_title = 'User' %>

<div class="user new">
  <h2>Create User</h2>

  <%= form_for(:user, :url => {:action => 'create'}) do |f| %>

    <%= render(:partial => 'form', :locals => {:f => f} ) %>

    <button class="btn btn-default" type="button">
      <%= submit_tag('Create User') %>
    </button>

  <% end %>
</div>
